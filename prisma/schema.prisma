// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


// MODELO EXAMPLE

model Orchestra {
  id Int @id @default(autoincrement())
  name String
  description String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Favorite {
  id Int @id @default(autoincrement())
}

model Reaction {
  id Int @id @default(autoincrement())
  reaction String
  image_url String
}

model Post_Reaction {
  id Int @id @default(autoincrement())
}

model Rol {
  id Int @id @default(autoincrement())
  rol String
}

model Orchestra_Member {
  isActive Boolean  @unique @default(true)
}

model Orchestra_Type {
  id Int @id @default(autoincrement())
  type String
}

model Type_Post {
  id Int @id @default(autoincrement())
  type String
}
